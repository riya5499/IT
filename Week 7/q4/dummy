CREATE TABLE [dbo].[WORKS] (
    [empName]     NCHAR (20) NOT NULL,
    [companyName] NCHAR (20) NOT NULL,
    [salary]      INT        NOT NULL,
    PRIMARY KEY CLUSTERED ([empName] ASC)
);

CREATE TABLE [dbo].[LIVES] (
    [empName] NCHAR (20) NOT NULL,
    [street]  NCHAR (10) NOT NULL,
    [city]    NCHAR (10) NOT NULL,
    PRIMARY KEY CLUSTERED ([empName] ASC)
);

