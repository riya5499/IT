CREATE TABLE [dbo].[Legends] (
    [id]       INT        NOT NULL,
    [name]     NCHAR (50) NOT NULL,
    [age]      NCHAR (10) NOT NULL,
    [category] NCHAR (50) NOT NULL,
    PRIMARY KEY CLUSTERED ([id] ASC)
);

