CREATE TABLE [dbo].[Items] (
    [itemID]  INT        NOT NULL,
    [flavour] NCHAR (50) NOT NULL,
    [price]   INT        NOT NULL,
    [taste]   NCHAR (50) NOT NULL,
    PRIMARY KEY CLUSTERED ([itemID] ASC)
);

