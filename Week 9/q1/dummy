CREATE TABLE [dbo].[Staff] (
    [Name]        VARCHAR (50) NOT NULL,
    [Age]         INT          NOT NULL,
    [Designation] VARCHAR (50) NOT NULL,
    [Salary]      INT          NOT NULL
);


