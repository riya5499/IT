CREATE TABLE [dbo].[Actors]
(
	[id] NCHAR(10) NOT NULL PRIMARY KEY, 
    [name] VARCHAR(50) NULL, 
    [age] NCHAR(10) NULL, 
    [category] VARCHAR(50) NOT NULL
);

